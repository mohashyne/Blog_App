<!-- User contents listed here -->
<div class="user-list">
  <ul>
 <% @users.each do |user| %>
<li>
 <% if user.photo.present? %>
  <div><img src="<%= user.photo %>" alt="user-image"></div>
<% else %>
  <div><img src="https://unsplash.com/photos/29pFbI_D1Sc" alt="default-image"></div>
<% end %>
<strong><%= user.name %></strong>
<p><%= user.bio%> </p>
<% end %>
   
  </ul>
</div>

