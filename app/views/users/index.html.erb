<!-- User contents listed here -->
<div class="user-list">
  <ul>
 <% @users.each do |user| %>
 <li><%= user.id %></li>
<li>
 <% if user.photo.present? %>
  <img style="width: 50px;" src="https://lordicon.com/icons/wired/flat/261-emoji-smile.svg" alt="user-image">
<% else %>
  <div><img src="https://unsplash.com/photos/29pFbI_D1Sc" alt="default-image"></div>
<% end %>
<strong><%= user.name %></strong>
<p><%= user.bio%> </p>
<p><%= user.created_at%> </p>
<% end %>
   
  </ul>
</div>

